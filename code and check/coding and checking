console.log(25 + 5);   
console.log(25 - 5);   
console.log(25 * 5);   
console.log(25 / 5);   

console.log(123 % 10);  //find the rem


console.log(1/0);
console.log(-1/0);
console.log(0/0);
console.log(123/123);



let first = "bodhisatta";
let last = "kumar";
