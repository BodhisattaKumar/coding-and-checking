console.log("hi paplu da");

//unary
console.log(typeof 123);
console.log(typeof asdf);
console.log(typeof false);

console.log(10 > 50);
console.log(10 < 50);
console.log(10 == 10);
console.log(10 != 10);
console.log(10 === 10);

//compare
console.log("apple" < "banana");
console.log("pen" < "apple");

//logical
console.log(true && false);
console.log(true || false);
console.log(!false);


let  num = 7878;
console.log(num % 2 == 0 ? "even" : "odd");
